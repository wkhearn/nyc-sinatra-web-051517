<h1>Figures</h1>
<a href="/figures/new" class="btn waves-effect waves-light teal lighten-2">Create Figure
  <i class="material-icons right">trending_flat</i>
</a><br>
<br>

<table class="highlight">
  <% @figures.each do |figure| %>
  <tr>
    <td>
      <a href="/figures/<%= figure.id %>"><%= figure.name %></a><br>
    </td>
  </tr>
  <% end %>
</table>

<br>
